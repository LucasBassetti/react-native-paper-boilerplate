{
  "extends": [
    "stylelint-config-recommended"
  ],
  "plugins": [
    "stylelint-react-native",
    "stylelint-order"
  ],
  "rules": {
		"order/order": [
			"custom-properties",
			"declarations"
		],
		"order/properties-alphabetical-order": true,
    "property-no-unknown": [true, {
      "ignoreProperties": ["content-visibility"]
    }]
	}
}
